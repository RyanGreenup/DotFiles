            "commands": ["workbench.action.toggleEditorWidths"]
        },
        {
            "before": ["<leader>", "w", "s"],
            "commands": ["workbench.action.splitEditorDown"]
        },
        {
            "before": ["<leader>", "t", "p"],
            "commands": ["workbench.action.terminal.focusPrevious"]
        },
        {
            "before": ["<leader>", "t", "n"],
            "commands": ["workbench.action.terminal.focusNext"]
        },
        {
            "before": ["<leader>", "w", "v"],
            "commands": ["workbench.action.splitEditor"]
        },
        {
            "before": ["<leader>", "w", "w"],
            "commands": ["workbench.action.focusNextGroup"]
        },
        {
            "before": ["<leader>", "w", "W"],
            "commands": ["workbench.action.focusPreviousGroup"]
        },
        {
            "before": ["<leader>", "x", "s"],
            "commands": ["editor.action.sortLinesAscending"]
        },
        {
            "before": ["<leader>", "x", "w"],
            "commands": ["editor.action.trimTrailingWhitespace"]
        }
