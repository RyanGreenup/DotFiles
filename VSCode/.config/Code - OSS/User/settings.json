        }
    ],
    "vim.easymotion": true,
    "vim.useSystemClipboard": true,
    "vim.easymotionMarkerFontSize": "50",
    "vim.easymotionMarkerFontFamily": "arial",
    "vim.easymotionMarkerForegroundColorOneChar": "fira",
    "vim.easymotionMarkerHeight": 20,
    "vim.foldfix": true,
    "powermode.shakeIntensity": 8,
    "[javascript]": {
        "editor.defaultFormatter": "mblode.pretty-formatter"
    },
    "markdownTags.autorefresh": true,
    "vsnotes.defaultNotePath": "~/Notes/MD/notes",
    "workbench.colorCustomizations": {},
    "gitlens.views.repositories.location": "gitlens",
    "gitlens.views.fileHistory.location": "gitlens",
    "gitlens.views.lineHistory.location": "gitlens",
    "gitlens.views.compare.location": "gitlens",
    "gitlens.views.search.location": "gitlens",
    "markdown.extension.print.includeVscodeStylesheets": false,
    "markdown.extension.syntax.plainTheme": true,
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "rewrap.reformat": true,
    "julia.enableTelemetry": true,
    "plantuml.render": "PlantUMLServer",
    "plantuml.server": "https://www.plantuml.com/plantuml",
    "markdownExtended.disabledPlugins": "kbd",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.codeActionsOnSave": null,
    "terminal.integrated.shell.linux": "/usr/bin/fish",
    "editor.fontFamily": "'Fira Code Medium', 'DejaVu Sans Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "emojisense.languages": {
        "markdown": true,
        "plaintext": false,
        "julia": true,
        "json": true,
        "scminput": true // language used in the source control commit message box

    },
    "git.autofetch": true,
    "workbench.colorTheme": "Dracula",
    "markdownlint.config": {
        "MD007": { "indent": 4 },
        "MD013": false
    },
    "python.dataScience.askForKernelRestart": false,
    "python.dataScience.alwaysTrustNotebooks": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.languageServer": "Microsoft",
    "diffEditor.renderSideBySide": true








